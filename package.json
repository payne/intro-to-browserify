  "bugs": {
    "url": "https://github.com/searsaw/intro-to-browserify/issues"
  },
  "homepage": "https://github.com/searsaw/intro-to-browserify#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "lite-server": "^2.2.0",
    "minifyify": "^7.3.4"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "handlebars": "^4.0.6",
    "jquery": "^3.1.1",
    "lodash": "^4.17.3"
